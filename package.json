{
  "name": "mojentic",
  "version": "0.1.0",
  "description": "Modern LLM integration framework for TypeScript with tool support and structured output generation",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint 'src/**/*.ts' 'examples/**/*.ts' --max-warnings 0",
    "lint:fix": "eslint 'src/**/*.ts' 'examples/**/*.ts' --fix",
    "format": "prettier --write 'src/**/*.ts' 'examples/**/*.ts'",
    "format:check": "prettier --check 'src/**/*.ts' 'examples/**/*.ts'",
    "audit": "npm audit --omit=dev --audit-level=moderate",
    "audit:fix": "npm audit fix",
    "quality": "npm run lint && npm run format:check && npm run test:coverage && npm run audit",
    "example:simple": "ts-node examples/simple_llm.ts",
    "example:structured": "ts-node examples/structured_output.ts",
    "example:tool": "ts-node examples/tool_usage.ts",
    "example:streaming": "ts-node examples/streaming.ts",
    "example:image": "ts-node examples/image_analysis.ts",
    "example:broker": "ts-node examples/broker_examples.ts",
    "example:broker-as-tool": "ts-node examples/broker_as_tool.ts",
    "example:tracer": "ts-node examples/tracer-demo.ts",
    "example:async-llm": "ts-node examples/async-llm.ts",
    "example:iterative-solver": "ts-node examples/iterative-solver.ts",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "keywords": [
    "llm",
    "ai",
    "openai",
    "ollama",
    "anthropic",
    "typescript",
    "agents"
  ],
  "author": "Stacey Vetzal <stacey@mojility.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "eslint-plugin-security": "^3.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vitepress": "^1.0.0-rc.31"
  },
  "dependencies": {
    "tiktoken": "^1.0.22",
    "zod": "^3.22.4"
  },
  "overrides": {
    "js-yaml": "3.14.2",
    "esbuild": "0.27.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/svetzal/mojentic-ts.git"
  }
}
